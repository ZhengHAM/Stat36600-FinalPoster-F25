dim(df)
colSums(is.na(df))
summary(df)
## Distribution of response variable
table(df$civil.war)
## Distribution of categorical predictor
table(df$dominance)
## Distribution of response variable
table(df$civil.war)
## Distribution of categorical predictor
table(df$dominance)
quant_vars <- df %>% select(exports, schooling, growth, concentration, lnpop,
fractionalization)
quant_long <- quant_vars %>%
pivot_longer(cols = everything(), names_to = "variable", values_to = "value")
# Faceted histogram plot
ggplot(quant_long, aes(x = value)) +
geom_histogram(bins = 30, fill = "lightgray", color = "black") +
facet_wrap(~ variable, scales = "free", ncol = 3) +
theme_minimal(base_size = 12) +
labs(title = "Distribution of Quantitative Predictors",
x = "Value",
y = "Count")
